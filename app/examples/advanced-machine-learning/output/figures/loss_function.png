<svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 600 400">
  <!-- Loss Function Training Curve -->
  <defs>
    <style>
      .axis { stroke: #333; stroke-width: 2; }
      .grid { stroke: #ddd; stroke-width: 0.5; }
      .train-loss { fill: none; stroke: #2196F3; stroke-width: 3; }
      .val-loss { fill: none; stroke: #FF5722; stroke-width: 3; }
      .legend-box { fill: white; stroke: #ccc; }
      .axis-label { font-family: Arial, sans-serif; font-size: 14px; text-anchor: middle; }
      .title { font-family: Arial, sans-serif; font-size: 16px; font-weight: bold; text-anchor: middle; }
    </style>
  </defs>
  
  <!-- Title -->
  <text x="300" y="30" class="title">Training Loss vs Validation Loss</text>
  
  <!-- Grid lines -->
  <g class="grid">
    <line x1="80" y1="80" x2="80" y2="350"/>
    <line x1="180" y1="80" x2="180" y2="350"/>
    <line x1="280" y1="80" x2="280" y2="350"/>
    <line x1="380" y1="80" x2="380" y2="350"/>
    <line x1="480" y1="80" x2="480" y2="350"/>
    
    <line x1="80" y1="350" x2="520" y2="350"/>
    <line x1="80" y1="290" x2="520" y2="290"/>
    <line x1="80" y1="230" x2="520" y2="230"/>
    <line x1="80" y1="170" x2="520" y2="170"/>
    <line x1="80" y1="110" x2="520" y2="110"/>
  </g>
  
  <!-- Axes -->
  <line x1="80" y1="350" x2="520" y2="350" class="axis"/>
  <line x1="80" y1="80" x2="80" y2="350" class="axis"/>
  
  <!-- Training Loss Curve (decreasing with some noise) -->
  <polyline points="80,320 120,290 160,260 200,240 240,220 280,210 320,200 360,195 400,190 440,188 480,186 520,185" 
            class="train-loss"/>
  
  <!-- Validation Loss Curve (decreasing then slightly increasing - overfitting) -->
  <polyline points="80,330 120,300 160,270 200,250 240,235 280,225 320,220 360,218 400,220 440,225 480,232 520,240" 
            class="val-loss"/>
  
  <!-- Axis Labels -->
  <text x="300" y="380" class="axis-label">Epoch</text>
  <text x="40" y="215" class="axis-label" transform="rotate(-90 40 215)">Loss</text>
  
  <!-- Tick labels -->
  <text x="80" y="370" style="font-size: 12px; text-anchor: middle;">0</text>
  <text x="180" y="370" style="font-size: 12px; text-anchor: middle;">25</text>
  <text x="280" y="370" style="font-size: 12px; text-anchor: middle;">50</text>
  <text x="380" y="370" style="font-size: 12px; text-anchor: middle;">75</text>
  <text x="480" y="370" style="font-size: 12px; text-anchor: middle;">100</text>
  
  <text x="70" y="350" style="font-size: 12px; text-anchor: end;">0.0</text>
  <text x="70" y="290" style="font-size: 12px; text-anchor: end;">0.5</text>
  <text x="70" y="230" style="font-size: 12px; text-anchor: end;">1.0</text>
  <text x="70" y="170" style="font-size: 12px; text-anchor: end;">1.5</text>
  <text x="70" y="110" style="font-size: 12px; text-anchor: end;">2.0</text>
  
  <!-- Legend -->
  <rect x="350" y="100" width="150" height="60" class="legend-box"/>
  <line x1="360" y1="115" x2="390" y2="115" class="train-loss"/>
  <text x="395" y="119" style="font-size: 12px;">Training Loss</text>
  <line x1="360" y1="140" x2="390" y2="140" class="val-loss"/>
  <text x="395" y="144" style="font-size: 12px;">Validation Loss</text>
  
  <!-- Annotations -->
  <circle cx="360" cy="218" r="3" fill="#FF5722"/>
  <text x="370" y="210" style="font-size: 10px; fill: #FF5722;">Early stopping point</text>
  
  <text x="450" y="260" style="font-size: 10px; fill: #666; font-style: italic;">Overfitting begins</text>
</svg>
